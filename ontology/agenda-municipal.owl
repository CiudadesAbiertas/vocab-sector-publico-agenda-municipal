###  http://schema.org/location
<http://schema.org/location> rdf:type owl:ObjectProperty ;
                             rdfs:domain <http://schema.org/Event> ;
                             rdfs:range <http://schema.org/Place> ;
                             rdfs:comment "The location of for example where the event is happening, an organization is located, or where an action takes place." ;
                             rdfs:isDefinedBy schema:location ;
                             rdfs:label "location" .


###  http://schema.org/name
<http://schema.org/name> rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <rdfs:label> ;
                         rdfs:domain <http://schema.org/Thing> ;
                         rdfs:range <http://schema.org/Text> .


###  http://schema.org/subEvent
<http://schema.org/subEvent> rdf:type owl:ObjectProperty ;
                             owl:inverseOf <http://schema.org/superEvent> ;
                             rdfs:domain <http://schema.org/Event> ;
                             rdfs:range <http://schema.org/Event> ;
                             rdfs:comment "An Event that is part of this event. For example, a conference event includes many presentations, each of which is a subEvent of the conference."@en ;
                             rdfs:isDefinedBy schema:subEvent ;
                             rdfs:label "subEvent"@en .


###  http://schema.org/superEvent
<http://schema.org/superEvent> rdf:type owl:ObjectProperty ;
                               rdfs:domain <http://schema.org/Event> ;
                               rdfs:range <http://schema.org/Event> ;
                               rdfs:comment "An event that this event is a part of. For example, a collection of individual music performances might each have a music festival as their superEvent."@en ;
                               rdfs:isDefinedBy schema:superEvent ;
                               rdfs:label "superEvent"@en .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#ambitoEvento
esagm:ambitoEvento rdf:type owl:ObjectProperty ;
                   rdfs:domain esagm:Evento ;
                   rdfs:range dbo:GovernmentalAdministrativeRegion ;
                   rdfs:comment "Territorial scope of an event."@en ,
                                "Ámbito territorial de un evento."@es ;
                   rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal> ;
                   rdfs:label "territorial scope"@en ,
                              "ámbito territorial"@es .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#canal
esagm:canal rdf:type owl:ObjectProperty ;
            rdfs:domain esagm:Evento ;
            rdfs:range [ owl:intersectionOf ( skos:Concept
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty skos:inScheme ;
                                                owl:hasValue <http://vocab.linkeddata.es/datosabiertos/kos/sector-publico/agenda-municipal/canal>
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ;
            rdfs:comment "Means for the realization of the event: face-to-face or online."@en ,
                         "Medio de realización del evento: presencial o en línea."@es ;
            rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal> ;
            rdfs:label "canal"@es ,
                       "channel"@en .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#documentacion
esagm:documentacion rdf:type owl:ObjectProperty ;
                    rdfs:domain esagm:Evento ;
                    rdfs:range esagm:Documentacion ;
                    rdfs:comment "Asociación con la documentación como actas, mociones, vídeos."@es ,
                                 "Association with documentation such as minutes, motions, videos."@en ;
                    rdfs:isDefinedBy "http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal" ;
                    rdfs:label "asociado con"@es ,
                               "associated with"@en .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#evento
esagm:evento rdf:type owl:ObjectProperty ;
             owl:inverseOf esagm:rolIntegrante ;
             rdfs:domain esagm:RolIntegranteEvento ;
             rdfs:range esagm:Evento ;
             rdfs:comment "Event in which a member participates with a role."@en ,
                          "Evento en el que participa un integrante con un rol."@es ;
             rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal> ;
             rdfs:label "event"@es ,
                        "evento"@en .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#integra
esagm:integra rdf:type owl:ObjectProperty ;
              rdfs:domain esagm:RolIntegranteEvento ;
              rdfs:range foaf:Agent ;
              rdfs:comment "El integrante de un evento con un cierto rol."@es ,
                           "The member of an event with a certain role."@en ;
              rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal> ;
              rdfs:label "integra"@es ,
                         "integrates"@en .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#rol
esagm:rol rdf:type owl:ObjectProperty ;
          rdfs:domain esagm:RolIntegranteEvento ;
          rdfs:range [ owl:intersectionOf ( skos:Concept
                                            [ rdf:type owl:Restriction ;
