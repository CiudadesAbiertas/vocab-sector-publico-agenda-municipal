###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#tipoEvento
esagm:tipoEvento rdf:type owl:ObjectProperty ;
                 rdfs:domain esagm:Evento ;
                 rdfs:range [ owl:intersectionOf ( skos:Concept
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty skos:inScheme ;
                                                     owl:hasValue <http://vocab.linkeddata.es/datosabiertos/kos/sector-publico/agenda-municipal/tipo-evento>
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
