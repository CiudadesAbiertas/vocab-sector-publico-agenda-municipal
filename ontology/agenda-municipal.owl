          rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal> ;
          rdfs:label "rol en evento"@es ,
                     "role in event"@en .


###  http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal#rolIntegrante
esagm:rolIntegrante rdf:type owl:ObjectProperty ;
                    rdfs:domain esagm:Evento ;
                    rdfs:range esagm:RolIntegranteEvento ;
                    rdfs:comment "Rol del integrante de un evento."@es ,
                                 "Role of a  member of an event."@en ;
                    rdfs:isDefinedBy <http://vocab.ciudadesabiertas.es/def/sector-publico/agenda-municipal> ;
                    rdfs:label "member role"@en ,
